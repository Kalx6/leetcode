class Solution:
    def addStrings(self, num1: str, num2: str) -> str:
        import sys
        sys.set_int_max_str_digits(0)
        x = [int(x) for x in num1 ]
        y = [int(x) for x in num2 ]

        a = int("".join(map(str,x)))

        b = int("".join(map(str,y)))

        return str(a+b)

        
        
