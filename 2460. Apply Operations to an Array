class Solution:
    def applyOperations(self, nums: List[int]) -> List[int]:
        for i in range(len(nums)-1):
            if nums[i] == nums[i+1]:
                nums[i] *=2
                nums[i+1] = 0
        a = nums.count(0)

        khal = [x for x in nums if x != 0]

        khal.extend([0]*a)

      
        
        
        return khal
